{
  "name": "@foodie/pretty-log",
  "version": "0.0.1",
  "description": "A prettier log for terminal.",
  "public": true,
  "main": "lib/umd/index.js",
  "module": "lib/es/index.js",
  "scripts": {
    "lint": "eslint src/**/*.ts",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "build": "rollup --config build/rollup.config.js",
    "release": "bash scripts/release.sh"
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Chersquwn/pretty-log.git"
  },
  "keywords": [
    "beautify",
    "pretty",
    "console",
    "log"
  ],
  "author": "Chersquwn <chersquwn@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Chersquwn/pretty-log/issues"
  },
  "homepage": "https://github.com/Chersquwn/pretty-log#readme",
  "dependencies": {
    "chalk": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@rollup/plugin-json": "^4.1.0",
    "@types/jest": "^26.0.14",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "conventional-changelog-cli": "^2.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.10.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "rollup": "^2.28.2",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-typescript2": "^0.27.3",
    "typescript": "^4.0.3"
  },
  "files": [
    "lib",
    "LICENSE",
    "package.json",
    "README.md"
  ]
}
